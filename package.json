{
  "name": "tabbable",
  "version": "4.0.0",
  "description": "Returns an array of all tabbable DOM nodes within a containing node.",
  "main": "dist/index.min.js",
  "module": "dist/index.esm.min.js",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile:esm": "cross-env BUILD_ENV=esm rollup -c",
    "compile:cjs": "cross-env BUILD_ENV=cjs rollup -c",
    "compile:umd": "cross-env BUILD_ENV=umd rollup -c",
    "compile": "npm run compile:esm && npm run compile:cjs && npm run compile:umd",
    "lint": "eslint .",
    "start": "npm run compile:cjs && budo -l -d -o test/debug.js -- -t brfs",
    "test": "karma start && npm run lint",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidtheclark/tabbable.git"
  },
  "author": "David Clark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidtheclark/tabbable/issues"
  },
  "homepage": "https://github.com/davidtheclark/tabbable#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-dev-expression": "^0.2.2",
    "brfs": "^2.0.2",
    "browserify": "^16.5.0",
    "budo": "^11.6.3",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "karma-browserify": "^7.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-mocha": "^1.3.0",
    "karma": "^4.4.1",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup": "^1.31.1",
    "watchify": "^3.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  }
}
